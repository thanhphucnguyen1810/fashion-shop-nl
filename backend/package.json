{
  "name": "fashion",
  "version": "1.0.0",
  "description": "BE fashion",
  "author": "thanhphucnguyen",
  "main": "src/server.js",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rmdir /s /q build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./build/src/server.js",
    "dev": "cross-env BUILD_MODE=development nodemon --exec babel-node ./src/server.js",
    "seed": "cross-env BUILD_MODE=development babel-node ./src/seeder.js"
  },
  "dependencies": {
    "@babel/runtime": "7.27.1",
    "@sendgrid/mail": "^8.1.6",
    "async-exit-hook": "^2.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.27.0",
    "nodemon": "^3.0.1"
  }
}
